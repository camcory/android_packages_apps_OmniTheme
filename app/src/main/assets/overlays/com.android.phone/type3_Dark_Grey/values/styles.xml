<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.Settings" parent="@android:style/Theme.Material">
        <item name="android:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>

    <style name="SettingsLight" parent="@android:style/Theme.Material">
        <item name="android:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <!-- Settings text. -->
        <item name="android:textColorPrimary">@*android:color/white</item>
        <!-- Settings description. -->
        <item name="android:textColorSecondary">@*android:color/white</item>
    </style>

    <style name="DialerSettingsLight" parent="@style/SettingsLight">
        <item name="android:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>

    <style name="DialerAlertDialogTheme" parent="@android:style/Theme.Material.Dialog">
        <item name="android:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>

    <style name="NetworkOperatorsSettingsTheme" parent="@android:style/Theme.Material">
        <item name="android:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>

    <style name="EmergencyDialerTheme" parent="@android:style/Theme.Material">
        <item name="android:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>

    <style name="SimImportTheme" parent="@android:style/Theme.Material" />

    <style name="Theme.Material.Settings" parent="@android:style/Theme.Material">
        <item name="android:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
</resources>
